{
  "name": "elite-models-gallery",
  "version": "2.0.0",
  "private": true,
  "description": "International models gallery built with React and Vite",
  "author": "Elite Models",
  "license": "MIT",
  "homepage": "https://nestaleks2.github.io/models/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestaleks2/models.git"
  },
  "keywords": [
    "models",
    "gallery",
    "react",
    "vite",
    "photos",
    "portfolio"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "serve": "vite preview --port 3000"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.1.0",
    "gh-pages": "^6.1.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "@eslint/js": "^8.56.0",
    "globals": "^13.24.0",
    "typescript": "^5.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}